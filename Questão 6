#include <stdio.h>

int main(){
    int pulo[5] = {2, 4, 6, 8, 10}

    printf("x = %d, \n", *(pulo + 2));
    printf("x = %d, \n", *(pulo + 4));
    printf("x = %d, \n", pulo + 4);
    printf("x = %d, \n", pulo + 2);
   
    //*(pulo + 2); // certo x = 6, 3 elemento
    //*(pulo + 4); // errado, x = 10, 5 elemento
    //pulo + 4; // errado, endereço 6356684
    //pulo + 2; // endereço 6356676
    
    return 0
}
